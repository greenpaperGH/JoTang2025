{"ast":null,"code":"import { ref, reactive } from 'vue';\nimport apiClient from '@/services/apiClient';\nexport default {\n  __name: 'OrderForm',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const order = reactive({\n      productId: null,\n      buyerId: null,\n      status: 'pending' // 默认状态\n    });\n    const error = ref(null);\n    const success = ref(null);\n    const handleSubmit = async () => {\n      error.value = null;\n      success.value = null;\n      try {\n        const result = await apiClient('/product-orders', 'POST', order);\n        if (result.code === 1) {\n          success.value = '订单创建成功！';\n          // 重置表单\n          order.productId = null;\n          order.buyerId = null;\n          order.status = 'pending';\n        } else {\n          error.value = result.msg;\n        }\n      } catch (err) {\n        error.value = err.message;\n      }\n    };\n    const __returned__ = {\n      order,\n      error,\n      success,\n      handleSubmit,\n      ref,\n      reactive,\n      get apiClient() {\n        return apiClient;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","apiClient","order","productId","buyerId","status","error","success","handleSubmit","value","result","code","msg","err","message"],"sources":["G:/vue-project/jotang-frontend/src/views/OrderForm.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>创建新订单</h2>\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div>\r\n        <label for=\"productId\">商品 ID:</label>\r\n        <input type=\"number\" v-model.number=\"order.productId\" id=\"productId\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"buyerId\">购买者 ID:</label>\r\n        <input type=\"number\" v-model.number=\"order.buyerId\" id=\"buyerId\" required>\r\n      </div>\r\n      <div>\r\n        <label for=\"status\">状态:</label>\r\n        <input type=\"text\" v-model=\"order.status\" id=\"status\" required>\r\n      </div>\r\n\r\n      <button type=\"submit\">创建订单</button>\r\n      <p v-if=\"error\" style=\"color: red;\">{{ error }}</p>\r\n      <p v-if=\"success\" style=\"color: green;\">{{ success }}</p>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive } from 'vue';\r\nimport apiClient from '@/services/apiClient';\r\n\r\nconst order = reactive({\r\n  productId: null,\r\n  buyerId: null,\r\n  status: 'pending' // 默认状态\r\n});\r\nconst error = ref(null);\r\nconst success = ref(null);\r\n\r\nconst handleSubmit = async () => {\r\n  error.value = null;\r\n  success.value = null;\r\n  try {\r\n    const result = await apiClient('/product-orders', 'POST', order);\r\n    if (result.code === 1) {\r\n      success.value = '订单创建成功！';\r\n      // 重置表单\r\n      order.productId = null;\r\n      order.buyerId = null;\r\n      order.status = 'pending';\r\n    } else {\r\n      error.value = result.msg;\r\n    }\r\n  } catch (err) {\r\n    error.value = err.message;\r\n  }\r\n};\r\n</script>"],"mappings":"AAyBA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,OAAOC,SAAS,MAAM,sBAAsB;;;;;;;IAE5C,MAAMC,KAAK,GAAGF,QAAQ,CAAC;MACrBG,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,MAAM,EAAE,SAAS,CAAC;IACpB,CAAC,CAAC;IACF,MAAMC,KAAK,GAAGP,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMQ,OAAO,GAAGR,GAAG,CAAC,IAAI,CAAC;IAEzB,MAAMS,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/BF,KAAK,CAACG,KAAK,GAAG,IAAI;MAClBF,OAAO,CAACE,KAAK,GAAG,IAAI;MACpB,IAAI;QACF,MAAMC,MAAM,GAAG,MAAMT,SAAS,CAAC,iBAAiB,EAAE,MAAM,EAAEC,KAAK,CAAC;QAChE,IAAIQ,MAAM,CAACC,IAAI,KAAK,CAAC,EAAE;UACrBJ,OAAO,CAACE,KAAK,GAAG,SAAS;UACzB;UACAP,KAAK,CAACC,SAAS,GAAG,IAAI;UACtBD,KAAK,CAACE,OAAO,GAAG,IAAI;UACpBF,KAAK,CAACG,MAAM,GAAG,SAAS;QAC1B,CAAC,MAAM;UACLC,KAAK,CAACG,KAAK,GAAGC,MAAM,CAACE,GAAG;QAC1B;MACF,CAAC,CAAC,OAAOC,GAAG,EAAE;QACZP,KAAK,CAACG,KAAK,GAAGI,GAAG,CAACC,OAAO;MAC3B;IACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module",